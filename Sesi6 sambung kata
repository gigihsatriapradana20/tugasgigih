#include <ctype.h>
#include <iostream>
#include <string.h>
using namespace std;
int main() {
  char T[20] = "Teknik";
  char I[20] = "Informatika";

  cout << " kalimat pertama adalah = ";
  cout << T;
  cout << endl;
  cout << " kalimat kedua adalah   = ";
  cout << I;
  cout << endl;
  strcat(T, I);
  cout << " Hasil Penggabungannya adalah = " << T;
  return 0;
}
